{
  "compilerOptions": {
    "allowJs": true,
    "declaration": true,
    "jsx": "react",
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es2020",
    "typeRoots": [
      "node_modules/@types",
      "typings"
    ]
  }
}
